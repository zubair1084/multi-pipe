pipeline {   
    agent any

    stages {   
        stage('master') { 
            steps { 
               sh 'echo "This is master branch"' 
            }
        }
     
        stage('sprint') { 
            steps { 
               sh 'echo "this is sprint application 1..."'
            }
        }

        stage('sprint1') { 
             steps { 
                sh 'echo "this is my sprint1 application........"'
            }
        }  
    }
}
