pipeline {   
    agent any

    stages {   
        stage('master') { 
            steps { 
               sh 'echo "This is master branch"' 
            }
        }
     
        stage('sprint1') { 
            steps { 
               sh 'echo "this is sprint1 application..."'
            }
        }

        stage('sprint2') { 
             steps { 
                sh 'echo "this is my sprint2 application........"'
            }
        }  
    }
}
